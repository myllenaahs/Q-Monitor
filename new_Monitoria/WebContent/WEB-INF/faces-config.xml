<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<navigation-rule>
		<display-name>new_Monitoria</display-name>
		<from-view-id>/pages/public/index.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{controllerBean.cadastrar}</from-action>
			<from-outcome>Sucesso</from-outcome>
			<to-view-id>/pages/secured/user/cadastro.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<application>
		<resource-bundle>
			<base-name>resources</base-name>
			<var>msgs</var>
		</resource-bundle>
	</application>

</faces-config>
